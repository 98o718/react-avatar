  "bugs": {
    "url": "https://github.com/sitebase/react-avatar/issues"
  },
  "keywords": [
    "component",
    "reactjs",
    "react-component",
    "avatar"
  ],
  "homepage": "https://github.com/sitebase/react-avatar",
  "peerDependencies": {
    "react": ">=0.14"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
